#!/usr/bin/env groovy
properties([
    [$class: 'GithubProjectProperty',
    displayName: '',
    projectUrlStr: 'https://github.com/saikadiyala1994/testingrepo.git/'],
    pipelineTriggers([githubPush()])])
    pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                sh "touch hello" 
                sh "touch hello2"
            }
        }
        stage('Test'){
            steps {
                echo 'make check'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'make publish'
            }
        }
    }
}
